{
  "name": "express-mongoose-es6-rest-api",
  "version": "5.0.0",
  "description": "A Boilerplate application for building REST APIs using express, mongoose in ES6 with code coverage",
  "author": "Kunal Kapadia <kunalkapadia12@gmail.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "nodemon",
    "start:debug": "cross-env DEBUG=express-mongoose-es6-rest-api:* yarn start",
    "lint": "esw --ext *.ts server --color",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "tsc",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"server/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"server/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:KunalKapadia/express-mongoose-es6-rest-api.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "4.16.4",
    "express-graphql": "^0.7.1",
    "express-jwt": "5.3.1",
    "express-validation": "false1.0.2",
    "express-winston": "3.0.1",
    "graphql": "^14.0.2",
    "graphql-compose": "false5.4.1",
    "graphql-compose-mongoose": "false5.2.1",
    "helmet": "3.15.0",
    "http-status": "1.3.1",
    "joi": "14.3.1",
    "jsonwebtoken": "8.4.0",
    "method-override": "^3.0.0",
    "mongoose": "5.4.3",
    "morgan": "1.9.1",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/bluebird": "3.5.25",
    "@types/body-parser": "1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "1.4.1",
    "@types/cors": "2.8.4",
    "@types/dotenv": "6.1.0",
    "@types/express": "4.16.0",
    "@types/express-graphql": "0.6.2",
    "@types/express-jwt": "false0.0.41",
    "@types/express-winston": "3.0.0",
    "@types/graphql": "14.0.4",
    "@types/helmet": "0.0.42",
    "@types/http-status": "0.2.30",
    "@types/joi": "14.0.1",
    "@types/jsonwebtoken": "false8.3.0",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "5.3.7",
    "@types/morgan": "1.7.35",
    "@types/node": "false10.12.18",
    "chai": "4.2.0",
    "commitizen": "^3.0.5",
    "coveralls": "^3.0.0",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "false3.4.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "false3.0.1",
    "eslint-watch": "4.0.2",
    "husky": "1.3.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "5.2.0",
    "nodemon": "false1.18.9",
    "prettier": "1.15.3",
    "supertest": "3.3.0",
    "supertest-as-promised": "4.0.2",
    "ts-node": "false7.0.1",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "false21.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "ext": "ts",
    "ignore": [
      "server/**/*.spec.ts"
    ],
    "exec": "ts-node ./server/index.ts"
  }
}
